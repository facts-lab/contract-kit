{
  "name": "@facts-kit/facts-token",
  "version": "1.0.0",
  "description": "Contract code for {name}",
  "type": "module",
  "main": "contract.js",
  "scripts": {
    "test": "npm run build && uvu test",
    "build": "rm -r dist; node ./scripts/build.js && cp initial-state.json dist/initial-state.json",
    "deploy": "npm run build && NODE_NO_WARNINGS=1 node ./scripts/deploy-contract-prod.mjs ./dist",
    "docs": "jsdoc ./src/ -c conf.json",
    "deploy:docs": "npm run build && NODE_NO_WARNINGS=1 node ./scripts/deploy-docs.mjs ./out",
    "generate-docs": "jsdoc --configure jsdoc.json --verbose"
  },
  "keywords": [],
  "author": "Forward Research",
  "license": "ISC",
  "devDependencies": {
    "@bundlr-network/client": "0.10.5",
    "arlocal": "1.1.60",
    "clean-jsdoc-theme": "4.2.7",
    "esbuild": "0.17.15",
    "jsdoc": "4.0.2",
    "npm": "9.6.4",
    "replace-in-file": "6.3.5",
    "uvu": "0.5.6",
    "warp-contracts": "1.3.3",
    "warp-contracts-plugin-deploy": "1.0.1"
  },
  "dependencies": {
    "@facts-kit/contract-kit": "0.4.2",
    "either-js": "1.0.0"
  }
}
